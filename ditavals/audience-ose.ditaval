<?xml version="1.0" encoding="UTF-8"?>
<val>
  <!-- set the default action to exclude all audience-specific elements -->
  <prop action="exclude" att="audience"/>
  
  <!-- include conditional body text and customized titles for the "sc" audience and child
    audiences. -->
  <prop action="include" att="audience" val="sc"/>
  <prop action="include" att="audience" val="uits"/>
  <prop action="include" att="audience" val="default"/>
</val>
